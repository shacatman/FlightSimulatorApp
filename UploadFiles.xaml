<Window x:Class="FlightSimulatorApp.UploadFiles"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlightSimulatorApp"
        mc:Ignorable="d"
        Title="UploadFiles" Height="450" Width="800">
    <StackPanel>
        <Label Margin="20" HorizontalAlignment="Center">Please Upload csv file:</Label>
        <Button x:Name="upload1" Content="browse" Click="Upload1_Click" Height="20" Width="50"/>
        <Button x:Name="Start" Content="Start" Click="Start_Click" Margin="100" Width="80"/>
    </StackPanel>
</Window>
